{
  "showProgressBar": "top",
  "title": "Survey New Design Test",
  "logo": "https://surveyjs.io/Content/Images/examples/image-picker/lion.jpg",
  "logoPosition": "left",
  "widthMode": "static",
  "pages": [
    {
      "name": "page1",
      "title": "Page 1 Title",
      "description": "This is the first page of test survey shown via survey-angular-ui module.",
      "questions": [
        {
          "type": "imagepicker",
          "name": "choosepicture",
          "title": "What animal would you like to see first ?",
          "choices": [
            {
              "value": "lion"
            }, {
              "value": "giraffe",
              "imageLink": "https://surveyjs.io/Content/Images/examples/image-picker/giraffe.jpg"
            }, {
              "value": "panda",
              "imageLink": "https://surveyjs.io/Content/Images/examples/image-picker/panda.jpg"
            }, {
              "value": "camel",
              "imageLink": "https://surveyjs.io/Content/Images/examples/image-picker/camel.jpg"
            }
          ]
        },
        {
          "type": "boolean",
          "name": "boolean",

          "labelTrue": "fewfewfwefwfqfewqfwqefwe",
          "labelFalse": "fewfewfwefwfqfewqfwqefwe"
        },
        {
          "type": "rating",
          "name": "nps_score",
          "title": "On a scale of zero to ten, how likely are you to recommend our product to a friend or colleague?",
          "isRequired": true,
          "rateMin": 0,
          "rateMax": 10,
          "minRateDescription": "(Most unlikely)",
          "maxRateDescription": "(Most likely)"
        },
        {
          "name": "dropdown",
          "type": "dropdown",
          "title": "Choose an item dropdown",
          "choices": [
            "item1",
            "item2",
            "item3"
          ]
        },
        {
          "name": "radiogroup",
          "type": "radiogroup",
          "title": "Choose an item",
          "choices": [
            "item1",
            "item2",
            "item3"
          ]
        },
        {
          "name": "name",
          "type": "text",
          "title": "Text",
          "enableIf": "{radiogroup} == 'item1'",
          "placeHolder": "Jon Snow"
        },
        {
          "name": "birthdate",
          "type": "text",
          "inputType": "date",
          "title": "Text Date"
        },
        {
          "name": "color",
          "type": "text",
          "inputType": "color",
          "title": "Text Color"
        },
        {
          "name": "email",
          "type": "text",
          "visibleIf": "{radiogroup} == 'item1'",
          "inputType": "email",
          "title": "Text Email",
          "placeHolder": "jon.snow@nightwatch.org",
          "validators": [
            {
              "type": "email"
            }
          ]
        }
      ]
    },
    {
      "name": "page2",
      "title": "Page 2",
      "questions": [
        {
          "name": "checkbox",
          "type": "checkbox",
          "title": "Choose item(s)",
          "choices": [
            "item1",
            "item2",
            "item3"
          ]
        },
        {
          "name": "surname",
          "type": "text",
          "title": "Surname",
          "placeHolder": "Snow"
        },
        {
          "name": "signature",
          "type": "signaturepad",
          "title": "Sign here"
        }
      ]
    }
  ]
}