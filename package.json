    "@typescript-eslint/parser": "^5.41.0",
    "axe-core": "^4.8.2",
    "axe-playwright": "^2.1.0",
    "axe-testcafe": "^3.0.0",
    "concurrently": "^7",
    "cross-env": "^7.0.3",
    "devextreme-screenshot-comparer": "^2.0.11",
    "eslint": "^7.32.0",
    "eslint-cli": "^1.1.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-surveyjs": "file:eslint-surveyjs",
    "get-func-name": "2.0.0",
    "husky": "^7.0.4",
    "node-uuid": "1.4.7",
    "playwright": "^1.51.1",
    "puppeteer": "22.13.1",
