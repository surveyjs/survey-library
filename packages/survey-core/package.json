{
  "name": "survey-core",
  "version": "1.8.33",
  "scripts": {
    "build": "webpack --env buildType=dev && webpack --env buildType=prod",
    "build:esm": "webpack --config ./webpack.esm.config.mjs --env buildType=dev",
    "build:i18n": "webpack --config ./webpack.i18n.config.js --env buildType=dev && webpack --config ./webpack.i18n.config.js --env buildType=prod",
    "build:themes": "webpack --config ./webpack.themes.config.js --env buildType=dev && webpack --config ./webpack.themes.config.js --env buildType=prod",
    "build:all": "npm run build && npm run build:i18n && npm run build:themes",
    "test": "karma start ./karma.conf.js --single-run",
    "test:watch": "karma start ./karma.conf.js"
  },
  "main": "survey.core.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/surveyjs/surveyjs.git"
  },
  "devDependencies": {
    "@types/lodash": "4.14.186",
    "@types/minimatch": "5.1.2",
    "@types/node": "7.0.4",
    "@types/papaparse": "^5.0.4",
    "@types/qunit": "2.0.31",
    "ace-builds": "1.2.2",
    "ajv": "8.17.1",
    "async": "3.2.5",
    "autoprefixer": "^10.4.17",
    "concurrently": "^5.2.0",
    "css-loader": "^7.1.2",
    "dotenv": "4.0.0",
    "eslint": "^7.32.0",
    "file-loader": "^0.10.1",
    "http-server": "^14.1.1",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-junit-reporter": "2.0.1",
    "karma-qunit": "4.1.2",
    "karma-sourcemap-loader": "0.3.8",
    "karma-webpack": "5.0.0",
    "merge-files": "^0.1.2",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "8.4.31",
    "postcss-calc": "9.0.1",
    "postcss-cli": "10.1.0",
    "postcss-fail-on-warn": "0.2.1",
    "qunitjs": "2.2.0",
    "replace-in-file": "^3.4.3",
    "rimraf": "2.5.4",
    "sass": "^1.62.1",
    "sass-loader": "^8.0.2",
    "standard-version": "^9.5.0",
    "style-loader": "^1.2.1",
    "surveyjs-doc-generator": "git+https://github.com/surveyjs/surveyjs-doc-generator.git",
    "svg-inline-loader": "^0.8.2",
    "testcafe": "^3.6.2",
    "ts-loader": "^8.0.15",
    "ts-node": "3.3.0",
    "typescript": "^4.8.0",
    "url-loader": "^4.1.1",
    "val-loader": "^0.5.1",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-merge": "^5.8.0",
    "webpack-remove-empty-scripts": "^1.0.4"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}