{
  "name": "survey-vue3-ui",
  "version": "2.0.0",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build-only": "vite build",
    "watch": "vite build --watch",
    "watch:dev": "vite build --watch --mode development",
    "build": "vite build && npm run build:types",
    "build:dev": "vite build --mode development",
    "build:types": "vue-tsc --project tsconfig.types.json --emitDeclarationOnly",
    "serve:example:dev": "npm run dev --prefix ./example",
    "serve:example:prod": "npm run preview --prefix ./example",
    "build:example:prod": "npm run build --prefix ./example",
    "type-check": "vue-tsc --project tsconfig.types.json --noEmit",
    "test": "vitest --no-watch --no-threads",
    "test:watch": "vitest --no-threads",
    "release": "commit-and-tag-version --message \"Release: %s [azurepipelines skip]\" ",
    "lint": "eslint ./src ./tests --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "testcafe": "testcafe ../../functionalTests/ --app \"npm run serve:example:prod\" --selector-timeout 1500 --env=vue3 --config-file ../../.testcaferc.dev.js",
    "testcafe:ci": "testcafe ../../functionalTests/ --app \"npm run serve:example:prod\" --selector-timeout 1500 --reporter minimal --env=vue3 --config-file ../../.testcaferc.js",
    "vrt": "testcafe ../../visualRegressionTests/ --app \"npm run serve:example:prod\" --screenshots ../../ --selector-timeout 1500 --env=vue3 --config-file ../../.testcaferc.dev.js",
    "vrt:ci": "testcafe ../../visualRegressionTests/ --app \"npm run serve:example:prod\" --screenshots ../../ --selector-timeout 1500 --reporter minimal --env=vue3 --config-file ../../.testcaferc.js",
    "accessibility-tests": "testcafe ../../accessibilityTests/ --app \"npm run serve:example:prod\" --screenshots ./ --selector-timeout 1500 --reporter minimal --env=vue3 --config-file ../../.testcaferc.dev.js",
    "accessibility-tests:ci": "testcafe ../../accessibilityTests/ --app \"npm run serve:example:prod\" --screenshots ./ --selector-timeout 1500 --reporter minimal --env=vue3 --config-file ../../.testcaferc.js"
  },
  "devDependencies": {
    "@babel/types": "^7.20.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@tsconfig/node16": "^16.1.3",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "2.4.4",
    "@vue/tsconfig": "^0.4.0",
    "commit-and-tag-version": "^11.0.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "16.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "survey-core": "../survey-core/build",
    "typescript": "^5.7.0",
    "vite": "^4.5.9",
    "vitest": "~0.32.4",
    "vitest-canvas-mock": "^0.3.3",
    "vue": "3.4.19",
    "vue-tsc": "^2.2.0"
  },
  "commit-and-tag-version": {
    "bumpFiles": [
      "package.json"
    ]
  }
}
