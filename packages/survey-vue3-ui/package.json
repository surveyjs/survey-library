{
  "name": "survey-vue3-ui",
  "version": "2.5.3",
  "homepage": "https://surveyjs.io/",
  "author": "DevSoft Baltic OU <info@devsoftbaltic.com>",
  "license": "MIT",
  "description": "A free MIT-licensed Vue 3 UI component that renders dynamic, interactive JSON-based forms and surveys. You can use it to collect responses from users and send them to your own database.",
  "keywords": [
    "vue",
    "vue3",
    "survey",
    "form",
    "surveyjs",
    "survey-library",
    "vue-component",
    "form-rendering",
    "survey-renderer",
    "dynamic-form",
    "interactive-form",
    "form-library",
    "form-management",
    "questionnaire",
    "data-collection",
    "data-validation",
    "form-validation",
    "input-validation",
    "ui-component",
    "json",
    "json-schema",
    "vue-schema-form",
    "survey-renderer",
    "client-side",
    "frontend",
    "javascript",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build-only": "vite build",
    "watch": "vite build --watch",
    "watch:dev": "vite build --watch --mode development",
    "build": "vite build && npm run build:types",
    "build:dev": "vite build --mode development",
    "build:types": "vue-tsc --project tsconfig.types.json --emitDeclarationOnly",
    "serve:example:dev": "npm run dev --prefix ./example",
    "serve:example:prod": "npm run preview --prefix ./example",
    "build:example:prod": "npm run build --prefix ./example",
    "type-check": "vue-tsc --project tsconfig.types.json --noEmit",
    "test": "vitest --no-watch --no-threads",
    "test:watch": "vitest --no-threads",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "accessibility-tests": "cross-env env=vue3 playwright test --project a11y --ui",
    "accessibility-tests:ci": "cross-env env=vue3 playwright test --project a11y --reporter dot --quiet",
    "e2e": "cross-env env=vue3 playwright test --project e2e --ui",
    "e2e:ci": "cross-env env=vue3 playwright test --project e2e",
    "scr": "cross-env env=vue3 playwright test --project vrt --ui",
    "scr:ci": "cross-env env=vue3 playwright test --project vrt",
    "release": "commit-and-tag-version --message \"Release: %s [azurepipelines skip]\" "
  },
  "devDependencies": {
    "@babel/types": "^7.20.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@tsconfig/node16": "^16.1.3",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "2.4.4",
    "@vue/tsconfig": "^0.4.0",
    "commit-and-tag-version": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "16.7.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "survey-core": "../survey-core/build",
    "typescript": "^5.7.0",
    "vite": "^4.5.9",
    "vitest": "~0.32.4",
    "vitest-canvas-mock": "^0.3.3",
    "vue": "3.4.19",
    "vue-tsc": "^2.2.0"
  },
  "commit-and-tag-version": {
    "bumpFiles": [
      "package.json"
    ]
  }
}
